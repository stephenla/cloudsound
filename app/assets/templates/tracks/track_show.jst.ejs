<div class="track-page row">
  <aside class="col-sm-4">
    <div class="page-header col-sm-4 col-sm-offset-4">
      <h4 class="header-center">
        <%= currentUser.get("username") %>
      </h4>
    </div>

  </aside>


  <div class="col-sm-8">
    <div class="page-header row">
      <h4 class="header-center">
        <%= track.get("title") %>
      </h4>
    </div>
    <div class="track-player row">

    </div>

    <div class="track-comments row">
      <div class="page-header row">
        <h4 class="header-center">
          Comments
        </h4>
      </div>
    </div>
  </div>

</div>
